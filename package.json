{
  "name": "@smarlhens/opinionated-safe-code-normalizer",
  "version": "1.1.0",
  "description": "TypeScript opinionated safe code normalizer using ts-morph & ESLint",
  "keywords": [
    "normalizer",
    "autofixer",
    "typescript",
    "eslint",
    "ts-morph"
  ],
  "homepage": "https://github.com/smarlhens/opinionated-safe-code-normalizer#readme",
  "bugs": {
    "url": "https://github.com/smarlhens/opinionated-safe-code-normalizer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smarlhens/opinionated-safe-code-normalizer.git"
  },
  "license": "MIT",
  "author": "Samuel MARLHENS <samuel.marlhens@gmail.com> (https://smarlhens.github.io)",
  "main": "src/index.ts",
  "bin": {
    "oscn": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "precommit": "lint-staged && npm run build",
    "prepare": "husky install",
    "prettier:check": "prettier --check ./**/*.ts",
    "prettier:write": "prettier --write ./**/*.ts",
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "eslint": "8.18.0",
    "ts-morph": "15.1.0",
    "ts-node": "10.8.1",
    "tslib": "2.4.0",
    "yargs": "17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/eslint": "8.4.3",
    "@types/node": "18.0.0",
    "@types/yargs": "17.0.10",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.3",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  }
}
